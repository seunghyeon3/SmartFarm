<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.smartFarm.grow.GrowMapper">

	<!-- 회원이메일을 통한 영농일지 조회 -->
	<select id="growList" parameterType="String" resultType="co.smartFarm.grow.GrowVO">
		SELECT DISTINCT KIT_NO FROM PUR_HIS
		WHERE MEM_EMAIL = #{memEmail}
	</select>

</mapper>