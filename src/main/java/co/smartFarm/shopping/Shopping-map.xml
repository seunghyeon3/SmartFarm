<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.smartFarm.shopping.ShoppingMapper">
	
	<select id="selectMemShoppingList" 
		parameterType="co.smartFarm.shopping.ShoppingVO"
		resultType="co.smartFarm.shopping.ShoppingVO">
		SELECT * FROM PLANT
		WHERE MEM_EMAIL = #{mem_email}
	</select>
	
</mapper>