<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.smartFarm.auction.NftMapper">
	<select id="selectNftMyList" parameterType="String" resultType="co.smartFarm.auction.NftVO">
		SELECT * FROM NFT WHERE MEM_EMAIL = #{memEmail} 
	</select>
	
	<select id="selectMyNft" parameterType="int" resultType="co.smartFarm.auction.NftVO">
		SELECT * FROM NFT WHERE NFT_NO = #{nftNo}
	</select>
</mapper>